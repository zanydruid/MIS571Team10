<?xml version="1.0" encoding="utf-8"?>
<manifest package="zanydruid.team10foodrecipe"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.RecipeActivity"
            android:parentActivityName=".activities.RecipeListActivity">

        </activity>
        <activity android:name=".activities.RecipeListActivity">

        </activity>
        <activity android:name=".activities.RecipeCreateActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

    </application>

</manifest>
